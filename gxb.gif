<script>
        var addEvent = (function() {
            if (document.addEventListener) {
                return function(el, type, fn) {
                    if (el.length) {
                        for (var i = 0; i < el.length; i++) {
                            addEvent(el[i], type, fn);
                        }
                    } else {
                        el.addEventListener(type, fn, false);
                    }
                };
            } else {
                return function(el, type, fn) {
                    if (el.length) {
                        for (var i = 0; i < el.length; i++) {
                            addEvent(el[i], type, fn);
                        }
                    } else {
                        el.attachEvent('on' + type,
                        function() {
                            return fn.call(el, window.event);
                        });
                    }
                };
            }
        })();
    
        var topNode = document.querySelector('.top');
        addEvent(topNode, 'click', function() {
            var _timer = setInterval(function () {
                var _top = document.documentElement.scrollTop || document.body.scrollTop;
                _top = Math.ceil(_top * 0.8);
                if (_top < 10) {
                    _top = 0;
                    clearInterval(_timer);
                }
                ;
                window.scrollTo(0, _top);
            }, 20);               
        });
        
        var bbs = document.querySelectorAll('.bbs');
        addEvent(bbs, 'click', function(e) {
            window._TRACK('bbsclick', {name: e.target.textContent}); 
        });
    </script>
